{
  "name": "mongodb-bank",
  "version": "1.0.0",
  "description": "Mongodb bank",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "author": "Stina Nerin",
  "type": "module",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "hvb-console": "^0.0.1",
    "mongodb": "^4.1.1",
    "parser": "^0.1.4",
    "vercel": "^30.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "vercel": {
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api"
      }
    ]
  }
}
